  <div class="barbox is-index">
    <div class="infobar">
      <div class="infobar__content">
        <div class="barlist jsAllCollection">
          {% for link in linklists['blog'].links %}
          <a href="{{link.url}}" class="barlist__link {% if link.active %} active {% endif %}">{{link.title}}</a>
          {% endfor %}
        </div>
        <!-- /.barlist -->
      </div>
    </div>
    <!-- /.infobar__inner -->
  </div>
<!-- /.infobar -->
{% comment %}<div class="divider"></div>{% endcomment %}


<div class="page-header">
	<div class="page-width">
		<h1 class="page-title is-small">{{ page.title }}</h1>
	</div>
</div>

<div class="page-wrap">
	<div class="page-width rte editor">
	  {{ page.content }}
	</div>	
</div>

